import java.util.Scanner;

public class Escribetunota2{
	public static void main(String args[]){
		Scanner entrada=new Scanner(System.in);
		System.out.println("escriba su nota");
		int tunotaes= entrada.nextInt();
    if(tunotaes==10 || tunotaes==9){
	    System.out.println("tu nota es sobresaliente");
	}
	else if  (tunotaes==8 || tunotaes==7){
		System.out.println("tu nota es notable");
		}
	
    else if (tunotaes==6){
		System.out.println("tu nota es bien");
		}
	else if (tunotaes==5){
		System.out.println("tu nota es suficiente");
		}
    else if (tunotaes<5){
    	System.out.println("estas suspendido");

    }



	

	}
 
 }
